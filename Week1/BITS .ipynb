   "source": [
    "231. Power of Two"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "id": "a36ba838",
   "metadata": {},
   "outputs": [],
   "source": [
    "class Solution(object):\n",
    "    def isPowerOfTwo(self, n):\n",
    "        if (n <= 0):\n",
    "            return n > 0;\n",
    "        while (n%2 == 0):\n",
    "            n/=2;\n",
    "        return (n&(n-1)) == 0"
   ]
  }
